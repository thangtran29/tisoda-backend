{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "service_group_name": {
      "type": "string"
    },
    "service_name": {
      "type": "string"
    },
    "duration": {
      "type": "integer"
    },
    "price": {
      "type": "biginteger"
    },
    "gallery": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "description": {
      "type": "blocks"
    },
    "place": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::place.place",
      "inversedBy": "services"
    },
    "appointment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::appointment.appointment",
      "mappedBy": "service"
    }
  }
}
